    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.0",
    "workbox-webpack-plugin": "^6.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "css-loader": "^6.8.1",
    "postcss-loader": "^7.3.3",
